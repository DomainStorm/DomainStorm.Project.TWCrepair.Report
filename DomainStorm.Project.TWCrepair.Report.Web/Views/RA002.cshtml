                                                                                    <td align="left" colspan="4" style="border-color:Black;border-width:1px;border-style:solid;white-space:nowrap;">
                                                                                        <table id="ctl19" cellspacing="0" cellpadding="0" style="height:10px;border-collapse:collapse;">
                                                                                            <tbody>
                                                                                                <tr>
                                                                                                    <td>@Html.CheckBoxFor(m => m.AccessoryEquipment_IsControlValve)<label for="ctl19_0">制水閥</label></td>
                                                                                                    <td>@Html.CheckBoxFor(m => m.AccessoryEquipment_IsOnGround)<label for="ctl19_1">地上消栓</label></td>
                                                                                                    <td>@Html.CheckBoxFor(m => m.AccessoryEquipment_IsUnderGround)<label for="ctl19_2">地下消栓</label></td>
