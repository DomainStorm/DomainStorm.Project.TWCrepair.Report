                                                                                                            @if (Model.PipeKind == pipeKind)
                                                                                                            { 
                                                                                                                <input type="checkbox" checked="checked"/>
                                                                                                            }
                                                                                                            else
                                                                                                            {
                                                                                                                <input type="checkbox"/>
                                                                                                            }
                                                                                                            <label >@pipeKind</label>
                                                                                                            </td>
                                                                                                        }
                                                                                                    }
                                                                                                    
                                                                                                </tr>
                                                                                                <tr>
                                                                                                    @{

