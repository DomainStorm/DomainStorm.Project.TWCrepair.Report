                                                                                                @if(i>0)
                                                                                                {
                                                                                                    <span>,</span>
                                                                                                }
                                                                                                @if(Model.DigFill_IsExcavator &&  Model.FixFormDigFill.ExcavatorItems != null && Model.FixFormDigFill.ExcavatorItems.Count() > i)
